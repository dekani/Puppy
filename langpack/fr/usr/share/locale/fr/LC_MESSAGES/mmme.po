#: mmme-bin.sh:20
msgid "MMME"
msgstr "MMME"

#: mmme-bin.sh:21
msgid "(manage main menu entries)"
msgstr "(gestion des entrées du menu principal)"

#: mmme-bin.sh:61
#, sh-format
msgid ""
"Version and authors of $AppTitle.\n"
"\n"
"- Version used: $AppVersion by $AuthorActualVersion.\n"
"- Previous versions: by $AuthorPreviousVersions.\n"
"- Original version: by $AuthorOriginalVersion."
msgstr ""
"Version et auteurs de $AppTitle.\n"
"\n"
"- Version utilisée : $AppVersion par $AuthorActualVersion.\n"
"- Versions précédentes : par $AuthorPreviousVersions.\n"
"- Version originale : par $AuthorOriginalVersion."

#: mmme-bin.sh:74
#: mmme-bin.sh:84
msgid "Please, select an entry in the list."
msgstr "Veuillez sélectionner une entrée dans la liste."

#: mmme-bin.sh:138
#, sh-format
msgid "Click on the entry you want to remove from the main menu of $DISTRO_NAME $DISTRO_VERSION."
msgstr "Cliquez sur l'entrée que vous souhaitez supprimer du menu principal de $DISTRO_NAME $DISTRO_VERSION."

#: mmme-bin.sh:139
#, sh-format
msgid "Click on the entry you want to restore in the main menu of $DISTRO_NAME $DISTRO_VERSION."
msgstr "Cliquez sur l'entrée que vous souhaitez restaurer au menu principal de $DISTRO_NAME $DISTRO_VERSION."

#: mmme-bin.sh:140
msgid "Click to sort items of the list"
msgstr "Cliquez pour classer les éléments de la liste"

#: mmme-bin.sh:148
#, sh-format
msgid "Manage"
msgstr "Gérer"

#: mmme-bin.sh:148
#, sh-format
msgid "main menu entries"
msgstr "les entrées du menu principal"

#: mmme-bin.sh:154
msgid "Current entries"
msgstr "Entrées actuelles"

#: mmme-bin.sh:172
msgid "Removed entries"
msgstr "Entrées supprimées"

#: mmme-bin.sh:193
#, sh-format
msgid "Version and authors of $AppTitle01."
msgstr "Version et auteurs de $AppTitle01."

#: mmme-bin.sh:199
#, sh-format
msgid "Refresh main menu and taskbar of $DISTRO_NAME $DISTRO_VERSION."
msgstr "Rafraîchir le menu principal et la barre des tâches de $DISTRO_NAME $DISTRO_VERSION."

#: mmme-bin.sh:204
#, sh-format
msgid "Quit $AppTitle01 and apply modifications."
msgstr "Quitter $AppTitle01 et valider les modifications."

#: mmme.sh:22
#, sh-format
msgid "$AppTitle is already running."
msgstr "$AppTitle est déjà activé."

#~ msgid "Quit $AppTitle01."
#~ msgstr "Quitter $AppTitle01."
