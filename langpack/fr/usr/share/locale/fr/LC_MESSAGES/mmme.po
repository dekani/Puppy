#: mmme-bin.sh:21
msgid "MMME"
msgstr "MMME"

#: mmme-bin.sh:22
msgid "(manage main menu entries)"
msgstr "(gestion des entrées du menu principal)"

#: mmme-bin.sh:62
#, sh-format
msgid ""
"Version and authors of $AppTitle.\n"
"\n"
"- Version used: $AppVersion by $AuthorActualVersion.\n"
"- Previous versions: by $AuthorPreviousVersions.\n"
"- Original version: by $AuthorOriginalVersion."
msgstr ""
"Version et auteurs de $AppTitle.\n"
"\n"
"- Version utilisée : $AppVersion par $AuthorActualVersion.\n"
"- Versions précédentes : par $AuthorPreviousVersions.\n"
"- Version originale : par $AuthorOriginalVersion."

#: mmme-bin.sh:75 mmme-bin.sh:85
msgid "Please, select an entry in the list."
msgstr "Veuillez sélectionner une entrée dans la liste."

#: mmme-bin.sh:139
#, sh-format
msgid ""
"Click on entries you want to remove from the main menu of $DISTRO_NAME "
"$DISTRO_VERSION."
msgstr ""
"Cliquez sur les entrées que vous souhaitez retirer du menu principal de "
"$DISTRO_NAME $DISTRO_VERSION."

#: mmme-bin.sh:140
#, sh-format
msgid ""
"Click on entries you want to restore in the main menu of $DISTRO_NAME "
"$DISTRO_VERSION."
msgstr ""
"Cliquez sur les entrées que vous souhaitez restaurer dans le menu principal "
"de $DISTRO_NAME $DISTRO_VERSION."

#: mmme-bin.sh:141
msgid "Click on entries to remove"
msgstr "Cliquez sur les entrées à retirer"

#: mmme-bin.sh:142
msgid "Click on entries to restore"
msgstr "Cliquez sur les entrées à restaurer"

#: mmme-bin.sh:150
#, sh-format
msgid "Manage"
msgstr "Gérer"

#: mmme-bin.sh:150
#, sh-format
msgid "main menu entries"
msgstr "les entrées du menu principal"

#: mmme-bin.sh:159
msgid "Current entries"
msgstr "Entrées actuelles"

#: mmme-bin.sh:190
msgid "Available entries"
msgstr "Entrées disponibles"

#: mmme-bin.sh:218
#, sh-format
msgid "Version and authors of $AppTitle01."
msgstr "Version et auteurs de $AppTitle01."

#: mmme-bin.sh:223
#, sh-format
msgid "Quit $AppTitle01."
msgstr "Quitter $AppTitle01."

#: mmme.sh:44
#, sh-format
msgid "$AppTitle is already running."
msgstr "$AppTitle est déjà activé."
